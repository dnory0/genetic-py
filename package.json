{
  "name": "genetic-py",
  "productName": "Genetic Py",
  "version": "0.1.2",
  "description": "A Simulator developed with ElectronJS for Genetic Algorithms written in Python",
  "homepage": "https://github.com/dnory0/genetic-py",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnory0/genetic-py.git"
  },
  "main": "app/main.js",
  "scripts": {
    "pyi-l": "python3 -m pip install pyinstaller --user",
    "pyi-w": "python -m pip install pyinstaller --user",
    "log": "git log --oneline --graph --all",
    "clean-l": "find dist/ -name '.fuse*' | xargs -d'\n' -I{} mv {} /media/$USER/*/.Trash-1000/files/; rm dist/* -rf; rm dist/.* -rf",
    "distpy-l": "pyinstaller --onefile --specpath app/python/linux/spec --distpath app/python/linux/dist --workpath app/python/linux/build app/python/ga.py",
    "distpy-w": "pyinstaller --onefile --specpath app/python/win/spec --distpath app/python/win/dist --workpath app/python/win/build app/python/ga.py",
    "cleanpy-l": "rm -r app/python/linux/build app/python/linux/spec app/python/__pycache__",
    "cleanpy-w": "rm -r app/python/win/build app/python/win/spec app/python/__pycache__",
    "updeps": "node update-deps.js",
    "start": "electron .",
    "tsc": "tsc",
    "dist-l": "electron-builder -linux --x64 --ia32",
    "dist-w": "electron-builder --win --x64 --ia32"
  },
  "keywords": [
    "genetic",
    "machine-learning"
  ],
  "author": "Hocine Abdellatif Houari <houarihocine99@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "highcharts": "^7.2.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.1",
    "@types/chart.js": "^2.8.5",
    "electron": "^6.0.8",
    "electron-builder": "^21.2.0",
    "typescript": "^3.6.3"
  }
}
