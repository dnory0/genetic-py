{
  "name": "genetic-py",
  "productName": "Genetic Py",
  "version": "0.3.1-dev1",
  "description": "A Simulator developed with ElectronJS for Genetic Algorithms written in Python",
  "homepage": "https://github.com/dnory0/genetic-py.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnory0/genetic-py.git"
  },
  "main": "app/main.js",
  "scripts": {
    "pyi-l": "python3 -m pip install pyinstaller --user",
    "pyi-w": "python -m pip install pyinstaller --user",
    "log": "git log --oneline --graph --all",
    "clean-l": "find dist/ -name '.fuse*' | xargs -d'\n' -I{} mv {} /media/$USER/*/.Trash-1000/files/; rm dist -rf",
    "clean-w": "rm dist -r",
    "distpy-l": "pyinstaller --onefile --specpath app/python/spec/linux --distpath app/python/dist/linux --workpath app/python/build/linux app/python/ga.py",
    "distpy-w": "pyinstaller --onefile --specpath app/python/spec/win --distpath app/python/dist/win --workpath app/python/build/win app/python/ga.py",
    "cleanpy": "rm -r app/python/build app/python/spec app/python/__pycache__",
    "updeps": "node update-deps.js",
    "start": "electron .",
    "watch": "tsc",
    "dist-l": "electron-builder --linux --x64",
    "dist-w": "electron-builder --win --x64"
  },
  "keywords": [
    "genetic",
    "machine-learning"
  ],
  "author": "Hocine Abdellatif Houari <houarihocine99@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "highcharts": "^7.2.0"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "typescript": "^3.6.4"
  }
}
