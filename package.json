{
  "name": "genetic-py",
  "productName": "Genetic Py",
  "version": "0.4.8-pre",
  "description": "A Simulator developed with ElectronJS for Genetic Algorithms written in Python",
  "homepage": "https://github.com/dnory0/genetic-py.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnory0/genetic-py.git"
  },
  "main": "app/main.js",
  "scripts": {
    "log": "git log --oneline --graph --all",
    "dev": "./node_modules/.bin/electron app/main.js --dev",
    "start": "electron .",
    "watch": "tsc",
    "dist": "./node_modules/.bin/electron-builder -l -w --ia32 --x64",
    "dist:w": "./node_modules/.bin/electron-builder -w --ia32 --x64",
    "dist:d": "yarn build --dir",
    "build": "./node_modules/.bin/electron-builder"
  },
  "keywords": [
    "genetic",
    "machine-learning"
  ],
  "author": "Hocine Abdellatif Houari <houarihocine99@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "highcharts": "^8.0.0"
  },
  "devDependencies": {
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2",
    "typescript": "^3.7.5"
  }
}
